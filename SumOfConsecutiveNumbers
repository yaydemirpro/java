public class SumOfConsecutiveNumbers {

	public static int sum(int firstNo, int lastNo){

		int result = 0;
		for (int i = firstNo; i <= lastNo; i++)
		    result += i;
		return result;
 	}


	public static void main(String[] args) {

		System.out.println("Sum of numbers from 0 to 10 is : " + sum(0, 10));
		System.out.println("Sum of numbers from 15 to 29 is : " + sum(15, 29));
		System.out.println("Sum of numbers from 100 to 200 is : " + sum(100, 200));
   	}
	
}
